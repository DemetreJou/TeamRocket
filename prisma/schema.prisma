generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model logs {
  log_id       Int      @id @default(autoincrement())
  machine_id   String[] @db.VarChar(36)
  time_created DateTime @db.Timestamp()
  time_added   DateTime @db.Timestamp() @default(now())
  level        Int      @db.SmallInt
  message      String[] @db.VarChar(500)
}
